{
  "name": "@etchteam/storybook-addon-a11y-interaction-tests",
  "version": "2.0.0",
  "description": "Storybook addon for accessibility interaction tests",
  "keywords": [
    "storybook-addons",
    "accessibility",
    "a11y",
    "storybook",
    "storybook-addon",
    "test",
    "interaction"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/etchteam/storybook-addon-a11y-interaction-tests"
  },
  "author": "Etch",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build:watch": "npm run clean && tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "prepare": "node .husky/install.mjs",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "storybook": {
    "displayName": "A11y Interaction Tests",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components"
    ]
  },
  "dependencies": {
    "focusable-selectors": "0.8.4",
    "postcss-selector-parser": "7.1.0",
    "shadow-dom-testing-library": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@etchteam/eslint-config": "^2.2.100",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^22.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-storybook": "10.2.8",
    "husky": "9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.7",
    "storybook": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
